<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ggymm.mtools.modules.logcat.LogcatController">
    <children>
        <GridPane vgap="8.0" AnchorPane.bottomAnchor="24.0" AnchorPane.leftAnchor="24.0" AnchorPane.rightAnchor="24.0" AnchorPane.topAnchor="24.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="720.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="48.0" minHeight="48.0" prefHeight="48.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="48.0" minHeight="48.0" prefHeight="48.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="48.0" minHeight="48.0" prefHeight="48.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="300.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <ComboBox fx:id="deviceList" minWidth="300.0" />
                        <Button fx:id="adbStart" minWidth="120.0" mnemonicParsing="false" text="启动adb服务">
                            <HBox.margin>
                                <Insets left="24.0" />
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="btn" />
                                <String fx:value="btn-primary" />
                            </styleClass>
                        </Button>
                        <Button fx:id="adbStop" minWidth="120.0" mnemonicParsing="false" text="停止adb服务">
                            <HBox.margin>
                                <Insets left="24.0" />
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="btn" />
                                <String fx:value="btn-primary" />
                            </styleClass>
                        </Button>
                        <Button fx:id="refreshDevice" minWidth="120.0" mnemonicParsing="false" text="刷新设备列表">
                            <HBox.margin>
                                <Insets left="24.0" />
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="btn" />
                                <String fx:value="btn-primary" />
                            </styleClass>
                        </Button>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.rowIndex="1">
                    <children>
                        <ComboBox fx:id="threadList" minWidth="300.0" />
                        <Button fx:id="refreshThread" minWidth="120.0" mnemonicParsing="false" text="刷新进程列表">
                            <HBox.margin>
                                <Insets left="24.0" />
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="btn" />
                                <String fx:value="btn-primary" />
                            </styleClass>
                        </Button>
                        <Button minWidth="120.0" mnemonicParsing="false" text="获取日志">
                            <HBox.margin>
                                <Insets left="24.0" />
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="btn" />
                                <String fx:value="btn-primary" />
                            </styleClass>
                        </Button>
                        <Button minWidth="120.0" mnemonicParsing="false" text="停止获取">
                            <styleClass>
                                <String fx:value="btn" />
                                <String fx:value="btn-primary" />
                            </styleClass>
                            <HBox.margin>
                                <Insets left="24.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <TextField fx:id="filter" maxWidth="1.7976931348623157E308" GridPane.rowIndex="2" />
                <ListView fx:id="console" fixedCellSize="0.0" prefHeight="300.0" GridPane.rowIndex="3" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
